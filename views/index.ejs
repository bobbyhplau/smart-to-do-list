  <div class="list-container">
    <ul class="list-group">
      <li class='list-titles list-group-item active'>Films / Series</li>
      <div id="toWatch"></div>
    </ul>
    <ul class="list-group">
      <li class='list-titles list-group-item active'>Restaurants, Cafes, Etc.</li>
      <div id="toEat"></div>
    </ul>
    <ul class="list-group">
      <li class='list-titles list-group-item active'>Books</li>
      <div id="toRead"></div>
    </ul>
    <ul class="list-group">
      <li class='list-titles list-group-item active'>Products</li>
      <div id="toBuy"></div>
    </ul>
